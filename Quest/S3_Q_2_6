int[] numbers = { 10, 20, 30, 40, 50 };
int length = numbers.Length;
int min = numbers[0];
int max = numbers[0];
for (int i = 0; 0 < length-i ; i++)
{
    if (min > numbers[i])
        min = numbers[i];
    if (max < numbers[i])
        max = numbers[i];
}
Console.WriteLine($"min = {min}, max ={max}");
