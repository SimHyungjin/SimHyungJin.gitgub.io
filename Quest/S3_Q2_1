int num = 1;
//1 - 1

for (int num2 = 1; num2 <= 100; num2++)
{
    if (num2 % 2 != 0)
        Console.WriteLine(num2);
}

//1 - 2


while (num <= 100)
{
    if (num % 2 != 0)
        Console.WriteLine(num);
    num++;
}

//1 - 3

num = 1;

do
{
    if (num % 2 != 0)
        Console.WriteLine(num);
    num++;
}
while (num <= 100);
